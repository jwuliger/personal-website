{
	"firestore": {
		"rules": "firestore.rules",
		"indexes": "firestore.indexes.json"
	},
	"hosting": {
		"public": "dist/jmw-site",
		"ignore": [
			"firebase.json",
			"**/.*",
			"**/node_modules/**"
		],
		"rewrites": [
			{
				"source": "**/*.@(css|js)",
				"destination": "/index.html"
			},
			{
				"source": "**",
				"function": "myserver"
			}
		]
	},
	"storage": {
		"rules": "storage.rules"
	}
}
